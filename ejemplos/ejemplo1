#Programa que pide al usuario palabras
#y devuelve si valida o no la palabra
#el automata m.
#cuando el usuario ingresa "exit" el programa termina

string s = "a*b|z+";
auto m = _s;

print "La expresion regular a utilizar es : " + s + "\n\n";



while(s not equals "exit"){
print "Ingrese una palabra";
	scan s;
	if(s not equals "exit")
	{
		print "Acepta:" + m accepts s;
	}	

}
